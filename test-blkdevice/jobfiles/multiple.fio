[global]
bs=${BSIZE}
iodepth=${DEPTH}
direct=1
ioengine=libaio
randrepeat=0
time_based
runtime=60
ramp_time=40
filesize=2G

[job1]
rw=${RW}
filename=/dev/nvme1n1
name=${RW}1
[job2]
rw=${RW}
filename=/dev/nvme1n2
name=${RW}2
[job3]
rw=${RW}
filename=/dev/nvme1n3
name=${RW}3
[job4]
rw=${RW}
filename=/dev/nvme1n4
name=${RW}4
